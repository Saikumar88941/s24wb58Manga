extends layout

block content
  h1= title
  - var image_names = ['abstract.jpg.jpg','modernwood.jpg','pic-3.jpg','pic2.jpg','realstic.jpg'] 
  - for(let i = 0;i < 3; i++)
    - let randomIndex = Math.floor(Math.random()*5) 
    img(src=`/images/${image_names[randomIndex]}`,alt=`${image_names[randomIndex]}`,height=300,width=300) 